doctype html
html
    head
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/style.css")
    body    
        h1 Contacts
        a(href="/update-cobj") Add to this table
        table(border="1")
            tr
                th Project Name
                th Client Name
                th Email
            each contact in records
                tr
                    td= contact.properties['project_name']
                    td= contact.properties['client_name']
                    td= contact.properties['email']
